<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin-package PUBLIC
	"-//Liferay//DTD Plugin Package 6.1.0//EN"
	"http://www.liferay.com/dtd/liferay-plugin-package_6_1_0.dtd">
<plugin-package>
	<name>${project.name}</name>
	<module-id>${groupId}/${artifactId}/${version}/${packaging}</module-id>
	<types>
		<type>layout-templates</type>
	</types>
	<tags>
		<tag>APE</tag>
	</tags>
	<short-description>${project.description}</short-description>
	<change-log>Initial migration to CMS</change-log>
	<page-url>${project.organization.url}</page-url>
	<author>${project.organization.name}</author>
	<licenses>
		<license osi-approved="true">LGPL</license>
	</licenses>
	<liferay-versions>
		<liferay-version>${liferay.version}+</liferay-version>
	</liferay-versions>
</plugin-package>