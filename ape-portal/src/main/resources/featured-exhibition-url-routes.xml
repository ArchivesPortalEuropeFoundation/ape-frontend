<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.1.0//EN"
"http://www.liferay.com/dtd/liferay-friendly-url-routes_6_1_0.dtd">
<routes>
	<route>
		<pattern>/a/{articleId:[^/]+}</pattern>
		<generated-parameter name="articleId">{articleId}</generated-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
	<route>
		<pattern>/pk/{classPK:[^/]+}</pattern>
		<generated-parameter name="classPK">{classPK}</generated-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
</routes>
